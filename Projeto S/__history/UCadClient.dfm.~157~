  object FDQry_Endereco: TFDQuery
    BeforeOpen = FDQry_EnderecoBeforeOpen
    BeforeInsert = FDQry_EnderecoBeforeInsert
    BeforeEdit = FDQry_EnderecoBeforeEdit
    AfterPost = FDQry_EnderecoAfterPost
    Connection = DM.FDConnection
    SQL.Strings = (
      'select'
      'cpf_cnpj,'
      'endereco,'
      'complemento, '
      'cep, '
      'bairro, '
      'cidade, '
      'uf'
      'from contratantes_endereco'
      'where cpf_cnpj = :cpf_cnpj'
      'order by cpf_cnpj')
    Left = 465
    Top = 210
    ParamData = <
      item
        Name = 'CPF_CNPJ'
        DataType = ftString
        FDDataType = dtAnsiString
        ParamType = ptInput
        Value = Null
      end>
  end
